{
  "extends": "tslint-config-standard",
  "rules": {
    "member-ordering": false,
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [
      true,
      "never"
    ],
    "jsdoc-format": false,
    "no-empty": true,
    "no-conditional-assignment": false,
    "space-before-function-paren": [true, "never"]
  }
}